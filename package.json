    "time-stamp": "^2.2.0",
    "title-case": "^2.1.1",
    "transform-loader": "^0.2.4",
    "webpack": "^4.36.1",
    "xml-js": "^1.6.11",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0"
