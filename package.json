    "is-utf8": "^0.2.1",
    "mustache": "^3.0.1",
    "mversion": "^1.13.0",
    "nanoid": "^2.0.3",
    "nanoid-dictionary": "^2.0.0",
    "p-each-series": "^2.1.0",
