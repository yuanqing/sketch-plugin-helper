{
  "name": "sketch-plugin-helper",
  "version": "1.0.0",
  "description": "A batteries-included toolkit for making Sketch plugins",
  "keywords": [
    "cli",
    "plugin",
    "sketch",
    "sketch-plugin-helper",
    "toolkit"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": "yuanqing/sketch-plugin-helper",
  "files": [
