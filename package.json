{
  "name": "sketch-plugin-helper",
  "version": "0.0.6",
  "description": "An opinionated, batteries-included toolkit for making Sketch plugins",
  "keywords": [
    "cli",
    "plugin",
    "sketch",
    "toolkit"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/sketch-plugin-helper.git"
  },
  "files": [
    "src/**/*.js",
    "src/template/**/*"
  ],
  "main": "src/api.js",
  "bin": {
    "sph": "src/cli-esm.js"
  },
  "scripts": {
    "fix": "prettier-standard 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.1",
    "dashify": "^2.0.0",
    "eslint": "^5.15.2",
    "eslint-config-sketch": "^0.2.4",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "esm": "^3.2.18",
    "fs-extra": "^7.0.1",
    "globby": "^9.1.0",
    "is-promise": "^2.1.0",
    "is-utf8": "^0.2.1",
    "mustache": "^3.0.1",
    "mversion": "^1.13.0",
    "nanoid": "^2.0.1",
    "nanoid-dictionary": "^2.0.0",
    "ora": "^3.2.0",
    "p-each-series": "^2.0.0",
    "prettier-standard": "^9.1.1",
    "quote-stream": "^1.0.2",
    "recursive-readdir": "^2.2.2",
    "run-series": "^1.1.8",
    "semver": "^5.6.0",
    "static-module": "^3.0.2",
    "temp-write": "^3.4.0",
    "time-stamp": "^2.2.0",
    "transform-loader": "^0.2.4",
    "webpack": "^4.29.4",
    "xml-js": "^1.6.11",
    "yargs": "^13.2.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  }
}
