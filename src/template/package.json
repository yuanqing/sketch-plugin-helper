{
  "name": "{{githubRepositoryName}}",
  "version": "0.0.0",
  "description": "{{pluginDescription}}",
  "keywords": [
    "plugin",
    "sketch",
    "sketch-plugin-helper"
  ],
  "license": "MIT",
  "author": "{{authorName}}",
  "repository": "{{githubUserName}}/{{githubRepositoryName}}",
  "devDependencies": {
    "sketch-plugin-helper": "*"
  },
  "scripts": {
    "build": "sph build",
    "fix": "sph lint --fix",
    "lint": "sph lint",
    "symlink": "sph symlink",
    "test": "sph test",
    "unlink": "sph symlink --delete",
    "version": "sph version",
    "watch": "sph build --watch"
  },
  "sketch-plugin-helper": {
    "name": "{{pluginName}}",
    "menu": [
      {
        "handler": "hello-world",
        "label": "Hello World"
      }
    ],
    "defaultSettings": {
      "greeting": "Hello",
      "name": "{{authorName}}"
    }
  }
}
