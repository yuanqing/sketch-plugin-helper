    "githubRepositoryName": "{{githubRepositoryName}}",
    "menu": [
      {
        "handler": "hello-world",
        "label": "Hello World"
      }
    ],
    "defaultSettings": {
      "greeting": "Hello",
      "name": "{{authorName}}"
    }
  }
